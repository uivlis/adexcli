{
  "name": "adexcli",
  "description": "An excellent CLI for AdEx",
  "version": "0.1.0",
  "author": "uivlis @uivlis",
  "bin": {
    "adexcli": "./bin/run"
  },
  "bugs": "https://github.com/uivlis/adexcli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "adex-protocol-eth": "^3.1.2",
    "cli-ux": "^5.2.1",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "ethers": "^4.0.29",
    "node-fetch": "^2.6.0",
    "web3": "^1.0.0-beta.55"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/uivlis/adexcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "adexcli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "uivlis/adexcli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
